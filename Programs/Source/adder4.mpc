from Compiler.circuit import Circuit

sb4 = sbits.get_type(4)
adder = Circuit('adder4')
a, b = [sbitvec(sb4.get_input_from(i)) for i in (0, 1)]
carry = sbits.get_type(1).get_input_from(1)

print_ln('Player 0: %s', a.elements()[0].reveal())
print_ln('Player 1: %s', b.elements()[0].reveal())
print_ln('carry: %s\n', carry.reveal())

res = adder(a, b, carry)
print_ln('Result %s', res[0].elements()[0].reveal())    # result
print_ln('Carry %s', res[1].elements()[0].reveal())     # carry
